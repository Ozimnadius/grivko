extends ../../../main

block variables
    - var title = 'Цены'

block content
    main.page
        //- desc
        section.page__desc
            .container
                .page-desc
                    h2.page-desc__title.title.title--line Цены на пошив мужских костюмов
                    .page-desc__breadcrumbs
                        include ../../../components/breadcrumbs/index
        //- prices
        section.page__prices.mb120
            .container
                .page-prices.content
                    each i in ["КОСТЮМЫ И СМОКИНГИ по эксклюзивному заказу","ПИДЖАКИ по эксклюзивному заказу","БРЮКИ по эксклюзивному заказу","ЖИЛЕТЫ по эксклюзивному заказу"]
                        include item
        //- signup
        section.page__signup
            .container
                include ../../../components/signup/index





